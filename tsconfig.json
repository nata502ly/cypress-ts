{
  "compilerOptions": {
    "baseUrl": "cypress", // This must be specified if "paths" is.
    "outDir": "./build/",
    "target": "es2019",
    "module": "commonjs",
    "removeComments": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "allowJs": true,
    "inlineSourceMap": true,
    "types": [
      "cypress"
    ],
    "typeRoots": ["./types"]
  }
}
